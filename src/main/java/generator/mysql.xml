<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<!-- <classPathEntry location="./generator/mysql-connector-java-5.1.6-bin.jar" /> -->
	<classPathEntry location="‪‪D://MavenStore//mysql//mysql-connector-java//5.1.6//mysql-connector-java-5.1.6.jar" />
	<context id="emerchant" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Criteria" />
		</plugin>
		
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin">
			<property name="suppressJavaInterface" value="false"/>
		</plugin>
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin4Criteria">
			<property name="suppressJavaInterface" value="false"/>
		</plugin>
		
		<plugin type="org.mybatis.generator.plugins.HumanReadableCommentPlugin"></plugin>
		
		<!-- <plugin type="org.mybatis.generator.plugins.MysqlKeyAutoIncrementPlugin"></plugin> -->
		
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://192.168.188.145:3306/qpm_2.0" userId="root"
						password="root">
			<!-- 针对mysql数据库 注解-->
    		<property name="useInformationSchema" value="true"></property>
		</jdbcConnection>
		<javaTypeResolver >
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		
		<!-- 自动生成代码的位置 -->
		<javaModelGenerator targetPackage="com.leaf.model" targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
		<sqlMapGenerator targetPackage="mapper"  targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.leaf.dao" targetProject="src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>


		<table schema="my" tableName="T_TEST"   domainObjectName="TTest"/>
	
	
	
	
	
	
	</context>
	
</generatorConfiguration>
